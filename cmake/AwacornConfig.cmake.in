@PACKAGE_INIT@
if(@USE_BOOST@)
  if(NOT Boost_FOUND)
    find_package(Boost REQUIRED COMPONENTS context)
  endif()
  if(NOT Boost_FOUND)
    message(FATAL_ERROR "Awacorn does not work properly without \"boost_context\". Please install package \"boost_context\" to continue.")
  endif()
  include_directories(${Boost_INCLUDE_DIRS})
  link_directories(${Boost_LIBRARY_DIRS})
  link_libraries(boost_context)
  add_definitions(-DAWACORN_USE_BOOST)
elseif(@USE_UCONTEXT@)
  add_definitions(-DAWACORN_USE_UCONTEXT)
endif()
include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")
set_and_check(Awacorn_INCLUDE_DIR "@PACKAGE_INCLUDE_INSTALL_DIR@")
check_required_components("@PROJECT_NAME@")
